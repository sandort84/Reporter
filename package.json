{
  "name": "Reporter",
  "version": "0.0.1",
  "main": "main.js",
  "description": "JIRA Work-Log Reporter Application",
  "author": "Tibor Sandor",
  "license": "MIT",
  "scripts": {
    "build": "better-npm-run build",
    "dev": "better-npm-run start-hot",
    "hot-server": "webpack-dev-server --config=webpack.config.development.js --hot --inline --colors"
  },
  "betterScripts": {
    "start": {
      "command": "electron ./",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start-hot": {
      "command": "electron ./",
      "env": {
        "HOT": 1,
        "NODE_ENV": "development"
      }
    },
    "package": {
      "command": "node package.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build": {
      "command": "webpack --config webpack.config.production.js --progress --profile --colors",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "bootstrap": "^3.3.7",
    "deep-equal": "^1.0.1",
    "electron-config": "^0.2.1",
    "font-awesome": "^4.7.0",
    "font-awesome-animation": "0.0.10",
    "keytar": "^3.0.2",
    "react": "^15.4.1",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.1",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "better-npm-run": "0.0.13",
    "css-loader": "^0.26.1",
    "electron": "1.4.14",
    "electron-devtools-installer": "^2.0.1",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "redux-logger": "^2.7.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "1.13.3",
    "webpack-dev-server": "^1.16.2"
  }
}
